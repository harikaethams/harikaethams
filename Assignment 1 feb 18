: def isprim(n):
    for i in range(1,n):
        if n%i ==0:
            return false
    return true 
    n = int(input('enter n:'))
    for i in range (1,n+1):
        if isprim(i):
            print(i,end=' ')
b=0
a=1
n=int(input("input a number please ")) 
for i in range (1,n+1):
    a=1
    for i in range (1,i+1):
        a=i*a
    b=b+a
